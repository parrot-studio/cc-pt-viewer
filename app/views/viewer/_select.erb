<div class="modal fade" id="select-modal" tabindex="-1" role="dialog" aria-labelledby="select-modal-label" aria-hidden="true">
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">&times; Close</span><span class="sr-only">Close</span></button>
        <h2 class="modal-title" id="share-modal-label">セット先選択</h2>
      </div>
      <div class="modal-body">
        <div class="row">
          <div class="col-sm-12 col-md-12">
            <div class="row">
              <div class="col-sm-6 col-md-6">
                <button type="button" class="btn btn-primary" id="select-btn-chain">
                  <span class="glyphicon glyphicon-link" aria-hidden="true"></span> 絆として追加する
                </button>
                <p class="clearfix"></p>
                <label>絆設定後</label>
                <div id="select-proposal-chain" class="col-sm-10 col-md-10"></div>
                <p class="clearfix"></p>
                <span id="select-chain-status" class="label"></span>
                <span id="select-union-status" class="label"></span>
              </div>
              <div class="col-sm-6 col-md-6">
                <button type="button" class="btn btn-info" id="select-btn-replace">
                  <span class="glyphicon glyphicon-user" aria-hidden="true"></span> メンバーとして置き換える
                </button>
                <p class="clearfix"></p>
                <label>置き換え後</label>
                <div id="select-proposal-replace" class="col-sm-10 col-md-10"></div>
              </div>
            </div>
          </div>
        </div>
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-default" data-dismiss="modal">
          <span class="glyphicon glyphicon-remove" aria-hidden="true"></span> セットしないで閉じる
        </button>
      </div>
    </div>
  </div>
</div>
<input type="hidden" id="select-position-key">
<input type="hidden" id="select-swap-position-key">
<input type="hidden" id="select-droped-code">
