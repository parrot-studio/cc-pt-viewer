{
    "env": {
        "browser": true
    },
    "extends": "eslint:recommended",
    "globals": {
        "$": false,
        "FastClick": false
    },
    "rules": {
        "indent": [
            "error",
            2
        ],
        "linebreak-style": [
            "error",
            "unix"
        ],
        "quotes": [
            "error",
            "double"
        ],
        "semi": [
            "error",
            "always"
        ]
    }
}
